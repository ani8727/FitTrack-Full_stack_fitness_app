# Admin Service environment
ADMIN_SERVICE_PORT=8082

# Postgres (Neon / Render)
SPRING_DATASOURCE_URL=jdbc:postgresql://your-neon-host:5432/fitness_admin
SPRING_DATASOURCE_USERNAME=admin_user
SPRING_DATASOURCE_PASSWORD=replace-me
JPA_DDL_AUTO=update
JPA_SHOW_SQL=false

# Auth0
AUTH0_ISSUER_URI=https://your-tenant.auth0.com/
AUTH0_JWK_SET_URI=https://your-tenant.auth0.com/.well-known/jwks.json
AUTH0_AUDIENCE=fitness_auth
