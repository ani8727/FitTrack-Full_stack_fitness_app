# AI Service environment
AI_SERVICE_PORT=8084

# MongoDB Atlas
SPRING_DATA_MONGODB_URI=mongodb+srv://ai_user:password@cluster.mongodb.net/ai_db?retryWrites=true&w=majority&appName=FitApp

# RabbitMQ / CloudAMQP
SPRING_RABBITMQ_ADDRESSES=amqps://user:password@host/vhost
SPRING_RABBITMQ_USERNAME=user
SPRING_RABBITMQ_PASSWORD=replace-me
SPRING_RABBITMQ_VIRTUAL_HOST=vhost

# Gemini API
GEMINI_API_URL=https://generativelanguage.googleapis.com
GEMINI_API_KEY=replace-me

# Auth0
AUTH0_ISSUER_URI=https://your-tenant.auth0.com/
AUTH0_JWK_SET_URI=https://your-tenant.auth0.com/.well-known/jwks.json
AUTH0_AUDIENCE=fitness_auth
